programa
{
	
	funcao inicio()
	{
	real n1, n2
		escreva("Digite o valor do primeiro número:")
		leia(n1)

         escreva("\nDigite o valor do segundo número:")
         leia(n2)

         se(n1 > n2){
         	escreva("\nO primeiro número é maior que o segundo",n1, n2)
         }
         senao se(n2 > n1){
         	escreva("\nO segundo número é maior que o primeiro",n2, n1)
         }
         senao{
         	escreva("Os números são iguais")
         }
	}
}
